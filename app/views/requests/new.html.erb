<div class="container d-flex justify-content-center my-5">

  <div class="card" style="height: 500px; width: 500px">
    <img class="card-img-top" src="<%=@bag.picture_url%>" alt="Card image cap">
    <div class="card-body">
      <p class="card-text"></p>
    </div>
      <%= link_to requests_path do %>
        <button type="submit" class="btn btn-outline-secondary" style="width: 100%; height: 50px">Rent</button>
      <% end %>
  </div>
</div>
